  <symbol name="token"/>
  <symbol name="language"/>
  <symbol name="IDREFS"/>
  <symbol name="ENTITIES"/>
  <symbol name="NMTOKEN"/>
  <symbol name="NMTOKENS"/>
  <symbol name="Name"/>
  <symbol name="NCName"/>
  <symbol name="ID"/>
  <symbol name="IDREF"/>
  <symbol name="ENTITY"/>
  <symbol name="integer"/>
  <symbol name="nonPositiveInteger"/>
  <symbol name="negativeInteger"/>
  <symbol name="long"/>      
  <symbol name="int"/>
  <symbol name="short"/>
  <symbol name="byte"/>
