  <symbol name="gYear"/>
  <symbol name="gMonthDay"/>
  <symbol name="gDay"/>
  <symbol name="gMonth"/>
  <symbol name="hexBinary"/>
  <symbol name="base64Binary"/>
  <symbol name="anyURI"/>
  <symbol name="QName"/>
