<assertion xml:id="value-of-a-defined-indicator-application"  type="theorem">
<CMP>
 Theorem name: "value-of-a-defined-indicator-application". <with style="linebreak"/>

 Formula (string): "forall(a:sets[uu],x:uu,x in a implies a(x)=an%individual)". <with style="linebreak"/>

 Formula (s-expression): 
"(forall (((uu unit%sort) a) (uu x))
 (implies (i-in x a) (= (apply-operator a x) an%individual)))". <with style="linebreak"/>

 Usage list: "elementary-macete", "transportable-macete". <with style="linebreak"/>
</CMP>

<FMP>
  <om:OMOBJ>
   <om:OMBIND>
    <om:OMS cd="the-kernel-theory" name="forall"/>
    <om:OMBVAR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMA>
        <om:OMS cd="the-kernel-theory" name="funsort"/>
         <om:OMS cd="indicators" name="uu"/>
         <om:OMS cd="the-kernel-theory" name="unit%sort"/>
       </om:OMA>
      </om:OMATP>
      <om:OMV name="a"/>
     </om:OMATTR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMS cd="indicators" name="uu"/>
      </om:OMATP>
      <om:OMV name="x"/>
     </om:OMATTR>

    </om:OMBVAR>

    <om:OMA>
     <om:OMS cd="the-kernel-theory" name="implies"/>
     <om:OMA>
      <om:OMS cd="the-kernel-theory" name="i-in"/>
      <om:OMV name="x"/>
      <om:OMV name="a"/>
     </om:OMA>
     <om:OMA>
      <om:OMS cd="the-kernel-theory" name="="/>
      <om:OMA>
       <om:OMV name="a"/>
       <om:OMV name="x"/>
      </om:OMA>
      <om:OMS cd="the-kernel-theory" name="an%individual"/>
     </om:OMA>
    </om:OMA>
   </om:OMBIND>

  </om:OMOBJ>
</FMP>

</assertion>


<assertion xml:id="meaning-of-indic-from-pred-element"  type="theorem">
<CMP>
 Theorem name: "meaning-of-indic-from-pred-element". <with style="linebreak"/>

 Formula (string): "forall(x:uu,p:[uu,prop],x in pred_to_indic(p) iff p(x))". <with style="linebreak"/>

 Formula (s-expression): 
"(forall ((uu x) ((uu prop) p))
 (iff (i-in x (predicate-to-indicator p)) (apply-operator p x)))". <with style="linebreak"/>

 Usage list: "elementary-macete", "transportable-macete". <with style="linebreak"/>
</CMP>

<FMP>
  <om:OMOBJ>
   <om:OMBIND>
    <om:OMS cd="the-kernel-theory" name="forall"/>
    <om:OMBVAR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMS cd="indicators" name="uu"/>
      </om:OMATP>
      <om:OMV name="x"/>
     </om:OMATTR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMA>
        <om:OMS cd="the-kernel-theory" name="funsort"/>
         <om:OMS cd="indicators" name="uu"/>
         <om:OMS cd="the-kernel-theory" name="prop"/>
       </om:OMA>
      </om:OMATP>
      <om:OMV name="p"/>
     </om:OMATTR>

    </om:OMBVAR>

    <om:OMA>
     <om:OMS cd="the-kernel-theory" name="iff"/>
     <om:OMA>
      <om:OMS cd="the-kernel-theory" name="i-in"/>
      <om:OMV name="x"/>
      <om:OMA>
       <om:OMS cd="the-kernel-theory" name="predicate-to-indicator"/>
       <om:OMV name="p"/>
      </om:OMA>
     </om:OMA>
     <om:OMA>
      <om:OMV name="p"/>
      <om:OMV name="x"/>
     </om:OMA>
    </om:OMA>
   </om:OMBIND>

  </om:OMOBJ>
</FMP>

</assertion>


<assertion xml:id="meaning-of-indic-from-sort-element"  type="theorem">
<CMP>
 Theorem name: "meaning-of-indic-from-sort-element". <with style="linebreak"/>

 Formula (string): "forall(x:uu,x in sort_to_indic{uu} iff #(x,uu))". <with style="linebreak"/>

 Formula (s-expression): 
"(forall ((uu x))
 (iff (i-in x (sort-to-indicator (undefined uu))) (is-defined-in-sort x uu)))". <with style="linebreak"/>

 Usage list: "elementary-macete", "transportable-macete". <with style="linebreak"/>
</CMP>

<FMP>
  <om:OMOBJ>
   <om:OMBIND>
    <om:OMS cd="the-kernel-theory" name="forall"/>
    <om:OMBVAR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMS cd="indicators" name="uu"/>
      </om:OMATP>
      <om:OMV name="x"/>
     </om:OMATTR>

    </om:OMBVAR>

    <om:OMA>
     <om:OMS cd="the-kernel-theory" name="iff"/>
     <om:OMA>
      <om:OMS cd="the-kernel-theory" name="i-in"/>
      <om:OMV name="x"/>
      <om:OMA>
       <om:OMS cd="the-kernel-theory" name="sort-to-indicator"/>
       <om:OMA>
        <om:OMS cd="the-kernel-theory" name="undefined"/>
        <om:OMS cd="indicators" name="uu"/>
        </om:OMA>

       </om:OMA>
      </om:OMA>
      <om:OMA>
       <om:OMS cd="the-kernel-theory" name="is-defined-in-sort"/>
       <om:OMV name="x"/>
       <om:OMS cd="indicators" name="uu"/>
