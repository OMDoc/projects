<assertion xml:id="prec%majorizes%characterization"  type="theorem">
<CMP>
 Theorem name: "prec%majorizes%characterization". <with style="linebreak"/>

 Formula (string): 
"forall(x:uu,s:[ind_1,uu],
  x prec%majorizes ran{s}
   iff 
  forall(n:ind_1,#(s(n)) implies s(n) prec x))". <with style="linebreak"/>

 Formula (s-expression): 
"(forall ((uu x) ((ind_1 uu) s))
 (iff (apply-operator prec%majorizes x (m-range s))
  (forall ((ind_1 n))
   (implies (is-defined (apply-operator s n))
    (apply-operator prec (apply-operator s n) x)))))". <with style="linebreak"/>

 Usage list: "elementary-macete", "transportable-macete". <with style="linebreak"/>
</CMP>

<FMP>
  <om:OMOBJ>
   <om:OMBIND>
    <om:OMS cd="the-kernel-theory" name="forall"/>
    <om:OMBVAR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMS cd="partial-order" name="uu"/>
      </om:OMATP>
      <om:OMV name="x"/>
     </om:OMATTR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMA>
        <om:OMS cd="the-kernel-theory" name="funsort"/>
         <om:OMS cd="pure-generic-theory-1" name="ind_1"/>
         <om:OMS cd="partial-order" name="uu"/>
       </om:OMA>
      </om:OMATP>
      <om:OMV name="s"/>
     </om:OMATTR>

    </om:OMBVAR>

    <om:OMA>
     <om:OMS cd="the-kernel-theory" name="iff"/>
     <om:OMA>
      <om:OMS cd="partial-order" name="prec%majorizes"/>
      <om:OMV name="x"/>
      <om:OMA>
