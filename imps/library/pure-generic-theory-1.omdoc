<assertion xml:id="definedness-of-dangling-conditionals"  type="theorem">
<CMP>
 Theorem name: "definedness-of-dangling-conditionals". <with style="linebreak"/>

 Formula (string): "forall(a:prop,b:ind_1,#(if(a, b, ?ind_1)) iff a)". <with style="linebreak"/>

 Formula (s-expression): "(forall ((prop a) (ind_1 b)) (iff (is-defined (if a b (undefined ind_1))) a))". <with style="linebreak"/>

 Usage list: "elementary-macete", "transportable-macete". <with style="linebreak"/>
</CMP>

<FMP>
  <om:OMOBJ>
   <om:OMBIND>
    <om:OMS cd="the-kernel-theory" name="forall"/>
    <om:OMBVAR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMS cd="the-kernel-theory" name="prop"/>
      </om:OMATP>
      <om:OMV name="a"/>
     </om:OMATTR>

     <om:OMATTR>
      <om:OMATP>
       <om:OMS cd="the-kernel-theory" name="sort"/>
       <om:OMS cd="pure-generic-theory-1" name="ind_1"/>
      </om:OMATP>
      <om:OMV name="b"/>
     </om:OMATTR>

    </om:OMBVAR>

    <om:OMA>
     <om:OMS cd="the-kernel-theory" name="iff"/>
     <om:OMA>
      <om:OMS cd="the-kernel-theory" name="is-defined"/>
      <om:OMA>
       <om:OMS cd="the-kernel-theory" name="if"/>
       <om:OMV name="a"/>
       <om:OMV name="b"/>
       <om:OMA>
        <om:OMS cd="the-kernel-theory" name="undefined"/>
        <om:OMS cd="pure-generic-theory-1" name="ind_1"/>
        </om:OMA>

       </om:OMA>
      </om:OMA>
      <om:OMV name="a"/>
     </om:OMA>
    </om:OMBIND>

  </om:OMOBJ>
</FMP>

</assertion>


<assertion xml:id="defined-iota-expression"  type="theorem">
