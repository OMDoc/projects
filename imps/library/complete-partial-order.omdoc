<axiom xml:id="prec-completeness">
<CMP>
 Axiom name: "prec-completeness". <with style="linebreak"/>

 Formula (string): 
"forall(p:[uu,prop],
  nonvacuous_q{p}
   and 
  forsome(alpha:uu,
    forall(theta:uu,p(theta) implies theta prec alpha))
   implies 
  forsome(gamma:uu,
    forall(theta:uu,p(theta) implies theta prec gamma)
     and 
    forall(gamma_1:uu,
      forall(theta:uu,p(theta) implies theta prec gamma_1)
       implies 
      gamma prec gamma_1)))". <with style="linebreak"/>

 Formula (s-expression): 
"(forall (((uu prop) p))
 (implies
  (and (nonvacuous? p)
   (forsome ((uu alpha))
    (forall ((uu theta))
     (implies (apply-operator p theta) (apply-operator prec theta alpha)))))
  (forsome ((uu gamma))
   (and
    (forall ((uu theta))
     (implies (apply-operator p theta) (apply-operator prec theta gamma)))
    (forall ((uu gamma_1))
     (implies
      (forall ((uu theta))
       (implies (apply-operator p theta) (apply-operator prec theta gamma_1)))
      (apply-operator prec gamma gamma_1)))))))". <with style="linebreak"/>

 Usage list: none. <with style="linebreak"/>
</CMP>

