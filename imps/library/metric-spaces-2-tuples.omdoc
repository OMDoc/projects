"(lambda (((pp_0 pp_1) phi) (rr r) ((pp_0 unit%sort) s))
 (and (apply-operator &lt; 0 r)
  (forall ((pp_0 x y))
   (implies (and (i-in x s) (i-in y s))
    (apply-operator &lt;=
     (apply-operator dist_1 (apply-operator phi x) (apply-operator phi y))
